<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.company.model.ProcesslistEntity" table="PROCESSLIST" schema="information_schema">
        <property name="id" column="ID"/>
        <property name="user" column="USER"/>
        <property name="host" column="HOST"/>
        <property name="db" column="DB"/>
        <property name="command" column="COMMAND"/>
        <property name="time" column="TIME"/>
        <property name="state" column="STATE"/>
        <property name="info" column="INFO"/>
    </class>
</hibernate-mapping>