<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.company.model.RoutinesEntity" table="ROUTINES" schema="information_schema">
        <property name="specificName" column="SPECIFIC_NAME"/>
        <property name="routineCatalog" column="ROUTINE_CATALOG"/>
        <property name="routineSchema" column="ROUTINE_SCHEMA"/>
        <property name="routineName" column="ROUTINE_NAME"/>
        <property name="routineType" column="ROUTINE_TYPE"/>
        <property name="dataType" column="DATA_TYPE"/>
        <property name="characterMaximumLength" column="CHARACTER_MAXIMUM_LENGTH"/>
        <property name="characterOctetLength" column="CHARACTER_OCTET_LENGTH"/>
        <property name="numericPrecision" column="NUMERIC_PRECISION"/>
        <property name="numericScale" column="NUMERIC_SCALE"/>
        <property name="characterSetName" column="CHARACTER_SET_NAME"/>
        <property name="collationName" column="COLLATION_NAME"/>
        <property name="dtdIdentifier" column="DTD_IDENTIFIER"/>
        <property name="routineBody" column="ROUTINE_BODY"/>
        <property name="routineDefinition" column="ROUTINE_DEFINITION"/>
        <property name="externalName" column="EXTERNAL_NAME"/>
        <property name="externalLanguage" column="EXTERNAL_LANGUAGE"/>
        <property name="parameterStyle" column="PARAMETER_STYLE"/>
        <property name="isDeterministic" column="IS_DETERMINISTIC"/>
        <property name="sqlDataAccess" column="SQL_DATA_ACCESS"/>
        <property name="sqlPath" column="SQL_PATH"/>
        <property name="securityType" column="SECURITY_TYPE"/>
        <property name="created" column="CREATED"/>
        <property name="lastAltered" column="LAST_ALTERED"/>
        <property name="sqlMode" column="SQL_MODE"/>
        <property name="routineComment" column="ROUTINE_COMMENT"/>
        <property name="definer" column="DEFINER"/>
        <property name="characterSetClient" column="CHARACTER_SET_CLIENT"/>
        <property name="collationConnection" column="COLLATION_CONNECTION"/>
        <property name="databaseCollation" column="DATABASE_COLLATION"/>
    </class>
</hibernate-mapping>