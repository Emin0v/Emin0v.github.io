<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.company.model.CollationsEntity" table="COLLATIONS" schema="information_schema">
        <property name="collationName" column="COLLATION_NAME"/>
        <property name="characterSetName" column="CHARACTER_SET_NAME"/>
        <property name="id" column="ID"/>
        <property name="isDefault" column="IS_DEFAULT"/>
        <property name="isCompiled" column="IS_COMPILED"/>
        <property name="sortlen" column="SORTLEN"/>
    </class>
</hibernate-mapping>